<%- include('../dpheader.ejs')%>
 

  <div class="main-container">
    <div class="signup-container">
      <h2>Sign Up</h2>
      <form class="signup-form" method="post" action="/dpsignup">
        <label for="dpName">Create Id</label>
        <input type="text" id="dpId" name="dpId" placeholder="Enter Id(A-Z and 0-0)">

        <label for="dpName">Your Name</label>
        <input type="text" id="dpName" name="dpName" placeholder="Enter Name">

        <label for="userPassword">Password</label>
        <input type="password" id="dpPassword" name="dpPassword" placeholder="Enter password">

        <label for="dpContact">Contact</label>
        <input type="tel" id="dpContact" name="dpContact" placeholder="Enter contact number">

        <button type="submit">Sign Up</button>
      </form>
    </div>
  </div>

<%- include('../footer.ejs')%>
