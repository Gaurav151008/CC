            <div class="card-quantity2">
                <div><a href="acceptorder/<%= fdcnames[i] %>/<%= neworders[i].orderId %>"><button type="submit">Accept</button></a></div>
            </div>
        </div>
        
        <% } %>
             
        <!-- Add more card elements here as needed -->
    </div>

</div>
